{
  "name": "@andenacitelli/credit-card-bonuses",
  "type": "module",
  "version": "1.0.0",
  "engines": {
    "node": "^20.0.0",
    "pnpm": "^9.0.0"
  },
  "bin": {
    "validate": "./scripts/validator/main.ts"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "sls offline start --stage offline",
    "deploy:prod": "sls deploy --stage prod",
    "lint": "pnpm lint:biome && pnpm lint:typescript",
    "lint:biome": "biome lint --apply .",
    "lint:typescript": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "vitest run",
    "generate": "redocly lint --extends=minimal src/api.yaml && openapi-zod-client ./src/api.yaml --output src/generated/api.client.ts --export-schemas",
    "prepare": "husky"
  },
  "dependencies": {
    "express": "~4.19.2",
    "express-async-errors": "~3.1.1",
    "helmet": "~7.1.0",
    "papaparse": "~5.4.1",
    "serverless-http": "~3.2.0",
    "zod": "~3.23.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.1",
    "@redocly/cli": "^1.12.0",
    "@tsconfig/recommended": "^1.0.6",
    "@types/express": "^4.17.21",
    "@types/node": "20.12.7",
    "@types/papaparse": "^5.3.14",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "~1.5.2",
    "@zodios/core": "^10.9.6",
    "chalk": "^5.3.0",
    "esbuild": "~0.20.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mkdirp": "^3.0.1",
    "openai": "^4.38.5",
    "openapi-zod-client": "^1.18.1",
    "puppeteer": "^22.7.1",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.52.1",
    "serverless-offline": "^13.4.0",
    "supertest": "^7.0.0",
    "tsx": "~4.7.3",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "~4.3.2",
    "vitest": "~1.5.2"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ],
    "*.ts": [
      "biome lint --apply"
    ]
  }
}
